{
    "configurations": [
        {
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}"
                ]
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "defines": [],
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/c++/8.1.1",
                "/usr/include/c++/8.1.1/x86_64-pc-linux-gnu",
                "/usr/include/c++/8.1.1/backward",
                "/usr/lib/gcc/x86_64-pc-linux-gnu/8.1.1/include",
                "/usr/local/include",
                "/usr/lib/gcc/x86_64-pc-linux-gnu/8.1.1/include-fixed",
                "/usr/include"
            ],
            "intelliSenseMode": "${default}",
            "name": "Linux",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}